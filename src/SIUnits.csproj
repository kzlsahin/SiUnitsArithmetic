<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>$(AssemblyName)Arithmetic</PackageId>
    <Title>SI Units Arithmetic</Title>
    <Authors>kzlsahin</Authors>
    <Product>$(AssemblyName)Arithmetic</Product>
    <Description>This package defines metric system units and provides related arithmetic operations.</Description>
    <Copyright>MIT License</Copyright>
    <Version>1.2.0.0</Version>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/kzlsahin/SiUnitsArithmetic</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>1.2.0.0</AssemblyVersion>
    <RepositoryType>git</RepositoryType>
    <PackageTags>SI Units</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Volume\**" />
    <EmbeddedResource Remove="Volume\**" />
    <None Remove="Volume\**" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Length\Area\" />
  </ItemGroup>

  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
