<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>$(AssemblyName)Arithmetic</PackageId>
    <Title>SI Units Arithmetic</Title>
    <Authors>kzlsahin</Authors>
    <Product>$(AssemblyName)Arithmetic</Product>
    <Description>SI units with arithmetic. for example: 1[m] * 2[m] = 2[m2]; nd 4[m2] / 4[m] = 1[m].  Only length and are types are included for now.</Description>
    <Copyright>MIT License</Copyright>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Length\Area\" />
    <Folder Include="Volume\" />
  </ItemGroup>

</Project>
