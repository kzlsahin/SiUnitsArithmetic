<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>$(AssemblyName)Arithmetic</PackageId>
    <Title>SI Units Arithmetic</Title>
    <Authors>kzlsahin</Authors>
    <Product>$(AssemblyName)Arithmetic</Product>
    <Description>SIUnitsArithmetic defines metric system units and provides related arithmetic operations including unit to unit multiplication even with compound units, resulting in higher or lower order units (mÂ², 1/m or m/s).
Supports length, mass and time units.</Description>
    <Copyright>MIT License</Copyright>
    <Version>1.2.3</Version>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/kzlsahin/SiUnitsArithmetic</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>1.2.3.0</AssemblyVersion>
    <RepositoryType>git</RepositoryType>
    <PackageTags>si units metric measurement quantity unit quantity convert conversion immutable </PackageTags>
    <PackageReleaseNotes>Conversion bug for higher order units is fixed,
Arithmetic performance is improved,
Documentation comments are added,
UnitConfig and UnitPrecission is added.
Equality checks values according to this precission if the precission is other then zero.
ToString methods are improved according to UnitPrecission. If precission is other then zero, values are formatted with "F[UnitPrecission]".
GethashCode methods are modified. Microsoftc.Bcl dependency is removed.
extension methods of generic conversions are renamed.</PackageReleaseNotes>
  <Configurations>Debug;Release</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Benchmark|AnyCPU'">
    <Optimize>True</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Length\Area\**" />
    <Compile Remove="Volume\**" />
    <EmbeddedResource Remove="Length\Area\**" />
    <EmbeddedResource Remove="Volume\**" />
    <None Remove="Length\Area\**" />
    <None Remove="Volume\**" />
  </ItemGroup>

  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
